{
  "schemaVersion": "0.6",
  "description": "profile description",
  "rules": [
    {
      "rule": "rule 1",
      "constraints": [
        {
          "anyOf":[
            {
              "field": "field1",
              "is": "longerThan",
              "value": 5
            },
            {
              "field": "field1",
              "is": "shorterThan",
              "value": 19
            }
          ]
        }
      ]
    }
  ],
  "fields": [
    {
      "name": "field1",
      "formatting": "%ggg"
    }
  ]
}