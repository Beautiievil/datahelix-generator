{
  "schemaVersion": "0.6",
  "fields": [
    {
      "name": "id",
      "unique": true
    },
    {
      "name": "number"
    }
  ],
  "rules": [
    {
      "rule": "rule 1",
      "constraints": [
        {
          "not": {
            "field": "id",
            "is": "null"
          }
        },
        {
          "not": {
            "field": "number",
            "is": "null"
          }
        },
        {
          "field": "id",
          "is": "ofType",
          "value": "integer"
        },
        {
          "field": "number",
          "is": "ofType",
          "value": "integer"
        },
        {
          "field": "id",
          "is": "greaterThanOrEqualTo",
          "value": 0
        }
      ]
    }
  ]
}