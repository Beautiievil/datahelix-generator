            "is": "null"
          }
        },
        {
          "not": {
            "field": "second",
            "is": "null"
          }
        },
        {
          "field": "first",
          "is": "ofType",
          "value": "datetime"
        },
        {
          "field": "second",
          "is": "ofType",
          "value": "datetime"
        },
        {
          "field": "first",
          "is": "after",
          "value": {
