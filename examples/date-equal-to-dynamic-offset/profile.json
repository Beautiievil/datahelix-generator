{
  "schemaVersion": "0.1",
  "fields": [
    {
      "name": "first"
    }, {
      "name": "second"
    }, {
      "name": "firstWorking"
    }, {
      "name": "secondWorking"
    }, {
      "name": "current"
    }
  ],
  "rules": [
    {
      "constraints": [
        {
          "not": {
