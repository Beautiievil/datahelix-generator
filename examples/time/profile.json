{
    "schemaVersion": "0.17",
    "description": "Time Profile",
    "fields": [
        {
            "name": "a",
            "type": "time",
            "nullable": false
        },
        {
            "name": "b",
            "type": "time",
            "nullable": false
        }
    ],
    "rules": [
        {
            "constraints": [
                {
                    "field": "a",
                    "afterOrAt": "00:00:00.000"
                },
                {
                    "if": {
                        "field": "a",
                        "after": "12:00:00"
                    },
                    "then" : {
                        "field": "b",
                        "equalTo": "12:34:56"
                    }   
                }      
            ]
        }
    ]
}
