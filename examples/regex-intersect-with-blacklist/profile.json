{
  "schemaVersion": "0.12",
  "fields": [
    {
      "name": "first_name",
      "type": "string",
      "nullable": false
    }
  ],
  "rules": [
    {
      "constraints": [
        {
          "field": "first_name",
          "matchingRegex": "(Joh?n|Mar[yk])"
        },
        {
          "field": "first_name",
          "matchingRegex": "M.*"
        },
        {
          "not": {
            "field": "first_name",
            "equalTo": "Mary"
          }
        }
      ]
    }
  ]
}