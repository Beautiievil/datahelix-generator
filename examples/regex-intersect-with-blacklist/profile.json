{
	"schemaVersion": "v3",
	"fields": [
		{
			"name": "first_name"
		}
	],
	"rules": [
		{ "constraints": [
			{ "not": { "field": "first_name", "is": "null" } },
			{ "field": "first_name", "is": "matchingRegex", "value": "(Joh?n|Mar[yk])" },
			{ "field": "first_name", "is": "matchingRegex", "value": "M.*" },
			{ "not": { "field": "first_name", "is": "equalTo", "value": "Mary" } }
		] }
	]
}
