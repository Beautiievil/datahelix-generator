{
	"schemaVersion": "v3",
	"description": "TestFactorCircular",
	"fields":
	[ 
		{"name": "A"},
		{"name": "B"}
	],
	"rules": 
	[		
		{ 
			"rule" : "d1",
			"constraints" : [
				{ 
					"if" : { "field" : "A", "is" : "null" },
					"then" : { "field" : "B", "is" : "null" } 
				}				
			]
		},
		{ 
			"rule" : "d2",
			"constraints" : [
				{ 
					"if" : { "field" : "B", "is" : "null" },
					"then" : { "field" : "A", "is" : "null" } 
				}
				
			]
		}
	]
}