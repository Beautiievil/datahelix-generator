{
    "schemaVersion": "0.1",

    "fields": [
		{ "name": "type" },
		{ "name": "x" },
		{ "name": "y" }
    ],
    "rules": [
		{ "if": { "field": "type", "equalTo": "a" },
		  "then": { "field": "x", "equalTo": "0.9" },
		  "else": { "field": "x", "is": "null" }},
		  
		{ "if": { "field": "type", "equalTo": "a" },
		  "then": { "field": "y", "equalTo": "12" },
		  "else": { "field": "y", "is": "null" }}
	]
}
